# Database Configuration
DB_TYPE=postgresql
DB_HOST=localhost
DB_PORT=5432
DB_NAME=citycircuit
DB_USER=citycircuit_user
DB_PASSWORD=citycircuit_password

# Database Connection Pool Settings
DB_POOL_SIZE=10
DB_MAX_OVERFLOW=20
DB_POOL_TIMEOUT=30
DB_POOL_RECYCLE=3600
DB_ECHO=false

# Flask Configuration
FLASK_DEBUG=true
FLASK_PORT=5000
JWT_SECRET=your-secret-key-here

# External API Keys (for future use)
GOOGLE_MAPS_API_KEY=your-google-maps-api-key
AZURE_MAPS_API_KEY=your-azure-maps-api-key
GEMINI_API_KEY=your-gemini-api-key