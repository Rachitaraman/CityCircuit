{
  "name": "city-circuit",
  "version": "1.0.0",
  "description": "Bus route optimization system for Mumbai's transportation challenges",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "setup:dev": "node scripts/setup-dev-db.js",
    "start:backend": "node scripts/mock-api-server.js",
    "start:all": "concurrently \"npm run start:backend\" \"npm run dev\"",
    "start:all:unix": "concurrently \"npm run start:backend\" \"npm run dev\"",
    "postinstall": "npm run setup:dev"
  },
