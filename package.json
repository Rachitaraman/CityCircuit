    "@tailwindcss/typography": "^0.5.19",
    "@types/crypto-js": "^4.2.2",
    "autoprefixer": "^10.4.16",
    "axios": "^1.6.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "express": "^5.2.1",
    "framer-motion": "^10.16.0",
    "i18next": "^25.7.3",
    "jsonwebtoken": "^9.0.2",
    "next": "^14.0.0",
    "next-i18next": "^15.4.3",
